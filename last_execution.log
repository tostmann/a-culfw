        b->zero.lowtime  = makeavg(b->zero.lowtime,  lowtime[CC_INSTANCE]);
     /* TODO: Receive other sync packages
      } else if(hightime[CC_INSTANCE] > lowtime[CC_INSTANCE]) {
        addbit(b, 1);*/
      } else {
        if(TX_REPORT & REP_MONITOR) DC('X');
        reset_input();
      }
      break;
  }
